{
  "name": "camera-controls",
  "version": "1.11.0",
  "author": "Yomotsu",
  "license": "MIT",
  "main": "dist/camera-controls.js",
  "jsnext:main": "dist/camera-controls.module.js",
  "module": "dist/camera-controls.module.js",
  "types": "types/camera-controls.d.ts",
  "repository": "yomotsu/camera-controls",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/runtime": "^7.3.1",
    "@types/three": "^0.93.18",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "4.3.2",
    "rollup-watch": "^4.3.1",
    "terser": "^3.14.1"
  },
  "scripts": {
    "dev": "rollup --config --watch",
    "build": "rollup --config && terser 'dist/camera-controls.js' -o 'dist/camera-controls.min.js' --comments '/^!/'"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": false
        }
      ]
    ]
  },
  "browserslist": [
    "last 2 versions",
    "ie >= 11"
  ],
  "keywords": [
    "three",
    "three.js",
    "orbit",
    "controls",
    "OrbitControls",
    "camera"
  ]
}
